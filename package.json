{
  "name": "telegram-gemini-bot",
  "version": "1.0.0",
  "description": "A Telegram bot to generate images using Google Gemini AI.",
  "main": "dist/bot.js",
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bot.js",
    "dev": "concurrently \"tsc -w\" \"node --watch dist/bot.js\"",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "keywords": [
    "telegram",
    "bot",
    "gemini",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "1.40.0",
    "dotenv": "17.2.4",
    "marked": "16.4.1",
    "node-telegram-bot-api": "0.67.0",
    "sqlite3": "5.1.7"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@jest/globals": "30.2.0",
    "@types/marked": "6.0.0",
    "@types/node": "25.2.2",
    "@types/node-telegram-bot-api": "0.64.13",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "db-migrate": "0.11.14",
    "db-migrate-sqlite3": "1.0.2",
    "jest": "29.7.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}